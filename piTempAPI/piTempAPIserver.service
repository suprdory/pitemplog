[Unit]
Description=uvicorn server for piTempAPI

[Service]
Restart=on-failure
RestartSec=5s
User=suprdory
Group=suprdory
WorkingDirectory=/home/suprdory/projects/piTempLog/piTempAPI
ExecStart=/home/suprdory/projects/piTempLog/piTempAPI/.venv/bin/uvicorn main:app --reload --port 8001 --host 192.168.1.200 --ssl-keyfile=./privkey.pem --ssl-certfile=./fullchain.pem

[Install]
WantedBy=multi-user.target
